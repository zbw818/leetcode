/**
 * Definition for singly-linked list.
 * struct ListNode {
 *     int val;
 *     ListNode *next;
 *     ListNode(int x) : val(x), next(NULL) {}
 * };
 */
class Solution {
public:
    ListNode *detectCycle(ListNode *head) {
        unordered_map<ListNode*,int>ump;
        while(head!=NULL){
            if(ump.count(head)){
                return head;
            }
            ump.insert(pair<ListNode*,int>(head,head->val));
            head=head->next;
        }
        return NULL;        
    }
};
